{
    "$schema": "https://typedoc.org/schema.json",
    "entryPoints": [
        "./src/main.ts"
    ],
    "plugin": [
        "typedoc-plugin-markdown"
    ],
    "out": "./docs/developers",
    "highlightLanguages": [
        "bash",
        "console",
        "css",
        "html",
        "javascript",
        "json",
        "jsonc",
        "json5",
        "tsx",
        "typescript"
    ],
    "cleanOutputDir": false,
    "theme": "default",
    "darkHighlightTheme": "dark-plus",
    "lightHighlightTheme": "light-plus",
    "exclude": ["**/?(*.)+(spec|test).+(ts|tsx|js)"]
}