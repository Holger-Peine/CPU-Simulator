{
    "$schema": "https://typedoc.org/schema.json",
    "entryPoints": [
        "./src/main.ts"
    ],
    "plugin": [
        "typedoc-plugin-markdown"
    ],
    "out": "./docs",
    "highlightLanguages": [
        "bash",
        "console",
        "css",
        "html",
        "javascript",
        "json",
        "jsonc",
        "json5",
        "tsx",
        "typescript"
    ],
    "githubPages": false,
    "cleanOutputDir": false
}